<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_square"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffff0"
    tools:context="com.goldfish.sevenseconds.activities.BarActivity">

    <com.ycl.tabview.library.TabView
        android:id="@+id/tabView_square"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#fffff0"
        app:tab_tabViewBackgroundColor="@color/shellowyellow"
        app:tab_textViewSize="0sp">
    </com.ycl.tabview.library.TabView>

</RelativeLayout>
